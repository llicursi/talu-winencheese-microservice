    <version>0.0.1-SNAPSHOT</version>
    <name>talu-client</name>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
    <developers>
        <developer>
            <id>tzinsly</id>
            <name>Tatiane Zinsly</name>
        </developer>
    </developers>
    <parent>
